
{% extends "base.html.twig" %}  

{% block body %}
<div class="container">
   <h2>
      Listado de telefonos
   </h2>
   <div>
      {% for iphone in iphone %}
      <div class="col-md-4">
         <div class="row">
            <div class="card">
               <h5> {{ iphone.nombre }} </h5>
               <img src=" {{iphone.img}} " alt="Imagenes de telefonos">
               <p> {{ iphone.descripcion }} </p>
               <a href="#" class="btn btn-primary">Mostrar mas objetos</a>
            </div>
         </div>
      </div>
      {% endfor %}
   </div>

</div>
{% endblock %}
